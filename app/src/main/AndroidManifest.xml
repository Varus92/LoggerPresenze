<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.loggerpresenze">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LoggerPresenze">
        <activity android:name=".views.PresenzeStudenti"></activity>
        <activity android:name=".views.ListaStudenti" />
        <activity android:name=".views.DashboardStudente" />
        <activity android:name=".views.Profilo" />
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".views.Dashboard" />
        <activity android:name=".views.Registrazione" />
    </application>

</manifest>